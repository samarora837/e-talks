	function notifyUser(debug) {
          var title;
          var options;
          if (!debug) {debug='';}

          title = 'AloProfe.com';
          options = {
            body: 'Tienes una nueva notificación '+debug,
            tag: 'AloProfe',
            icon: 'http://soporte.aloprofe.com/flat-visual-chat/console/include/files/pp.jpeg'
          };
          if(!document.hasFocus()){
              if ('Notification' in window) {    
                  Notification.requestPermission(function() {
                    var notification = new Notification(title, options);
                    notification.onclick = function(x) { window.focus(); this.close(); };
                  });
               }else{
                  alert(title + ' - '+options.body );
               } 
           }    
	}
</script>
